    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-define": "^1.3.1",
    "babel-plugin-transform-require-ignore": "^0.1.1",
